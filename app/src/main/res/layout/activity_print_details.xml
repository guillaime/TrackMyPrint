<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.tmp.trackmyprint.MainActivity">

    <ImageView
        android:layout_width="0dp"
        app:srcCompat="@color/colorProfileRectangle"
        android:id="@+id/top"
        android:layout_height="175dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_margin="0dp"
        app:layout_constraintHorizontal_bias="0.0"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:srcCompat="@drawable/profileimage"
        android:id="@+id/profile_image"
        app:layout_constraintBottom_toBottomOf="@+id/top"
        app:layout_constraintLeft_toLeftOf="@+id/top"
        android:layout_marginStart="90dp"
        android:layout_marginLeft="90dp"
        app:layout_constraintTop_toTopOf="@+id/top"
        app:layout_constraintVertical_bias="0.29000002" />

    <TextView
        android:text="Luuk Hermans"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/profile_name"
        android:textColor="@android:color/background_light"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/profile_image"
        android:layout_marginLeft="8dp"
        app:layout_constraintTop_toTopOf="@+id/profile_image" />

    <TextView
        android:text="Last check at 5.15 pm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/last_check_in"
        app:layout_constraintBottom_toBottomOf="@+id/profile_image"
        android:textColor="@android:color/background_light"
        app:layout_constraintRight_toRightOf="@+id/top"
        app:layout_constraintHorizontal_bias="0.0"
        android:layout_marginStart="8dp"
        app:layout_constraintLeft_toRightOf="@+id/profile_image"
        android:layout_marginLeft="8dp"
        android:layout_marginBottom="6dp" />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="33dp"
        app:srcCompat="@drawable/checkin_status"
        android:id="@+id/check_in_status"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/profile_image"
        app:layout_constraintLeft_toLeftOf="@+id/top"
        app:layout_constraintRight_toRightOf="@+id/top"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="68dp"
        android:layout_marginEnd="68dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginLeft="68dp"
        android:layout_marginRight="68dp"
        app:layout_constraintBottom_toBottomOf="@+id/top"
        app:layout_constraintVertical_bias="0.0" />


    <android.support.v7.widget.CardView
        android:layout_width="0dp"
        app:layout_constraintTop_toBottomOf="@+id/top"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        android:layout_height="50dp"
        android:id="@+id/cardView">

        <android.support.constraint.ConstraintLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                app:srcCompat="@drawable/printer_solid"
                android:id="@+id/imageView7"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/imageView6"
                android:layout_width="30dp"
                android:layout_height="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="16dp"
                android:layout_marginLeft="16dp" />

            <ImageView
                app:srcCompat="@drawable/triangle"
                android:id="@+id/imageView6"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_height="10dp"
                android:layout_width="10dp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp" />

            <TextView
                android:text="Printing sector"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView3"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView7"
                android:layout_marginLeft="16dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginRight="16dp"
                app:layout_constraintHorizontal_bias="0.14"
                app:layout_constraintVertical_bias="0.6" />

        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>

    <android.support.v7.widget.CardView
        android:layout_width="0dp"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintLeft_creator="1"
        android:id="@+id/cardView3"
        android:layout_height="295dp"
        android:layout_marginTop="8dp">

        <android.support.constraint.ConstraintLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                app:srcCompat="@drawable/imageback"
                android:id="@+id/imageView13"
                android:layout_marginEnd="72dp"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginRight="72dp"
                app:layout_constraintBottom_toBottomOf="parent" />

            <ImageView
                android:layout_width="100dp"
                android:layout_height="100dp"
                app:srcCompat="@drawable/imagefront"
                android:id="@+id/imageView12"
                android:layout_margin="0dp"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp"
                app:layout_constraintRight_toLeftOf="@+id/imageView13"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                app:layout_constraintHorizontal_bias="0.9"
                app:layout_constraintBottom_toBottomOf="parent" />

            <TextView
                android:text="Preview"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView10"
                app:layout_constraintTop_toBottomOf="@+id/linearLayout"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="16dp" />

            <TextView
                android:text="Print amount: 500x"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView9"
                android:layout_marginTop="10dp"
                app:layout_constraintTop_toBottomOf="@+id/textView8"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView11"
                android:layout_marginLeft="16dp" />

            <ImageView
                android:layout_width="10dp"
                app:srcCompat="@drawable/blue_oval"
                android:id="@+id/imageView11"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView10"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp"
                android:layout_height="10dp" />

            <TextView
                android:text="Paper color: "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView8"
                app:layout_constraintTop_toBottomOf="@+id/textView6"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView10"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="9dp" />

            <ImageView
                android:layout_height="10dp"
                app:srcCompat="@drawable/blue_oval"
                android:id="@+id/imageView10"
                android:layout_width="10dp"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/imageView9"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp" />

            <TextView
                android:text="Details"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView7"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp" />

            <TextView
                android:text="Paper size: A4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView6"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toRightOf="@+id/imageView9"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />

            <ImageView
                app:srcCompat="@drawable/blue_oval"
                android:id="@+id/imageView9"
                android:layout_width="10dp"
                android:layout_height="10dp"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/textView7"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp" />

            <LinearLayout
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/textView9"
                android:layout_marginEnd="16dp"
                app:layout_constraintRight_toRightOf="parent"
                android:layout_marginRight="16dp"
                android:layout_marginStart="16dp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="16dp"
                android:background="@color/cardview_shadow_start_color"
                android:layout_height="1dp"
                android:id="@+id/linearLayout" />
        </android.support.constraint.ConstraintLayout>
    </android.support.v7.widget.CardView>


    <!--<Button-->
    <!--android:text="UserList"-->
    <!--android:layout_width="wrap_content"-->
    <!--android:layout_height="wrap_content"-->
    <!--tools:layout_editor_absoluteY="11dp"-->
    <!--tools:layout_editor_absoluteX="0dp"-->
    <!--android:id="@+id/toUserListBtn" />-->

</android.support.constraint.ConstraintLayout>
